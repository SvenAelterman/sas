{
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET"
            ],
            "allowedRoles": [
                "registeredusers"
            ]
        },
        {
            "route": "/api/*",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/login",
            "rewrite": "/.auth/login/aad"
        }
    ],
    "responseOverrides": {
        "401": {
            "redirect": "/login",
            "statusCode": 302
        }
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}